{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/embed/widget.txt",
      "dest": "/embed/widget.txt",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/embed/widget-loader.js",
      "dest": "/embed/widget-loader.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    { "handle": "filesystem" }
  ]
}
