{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    {
      "src": "/embed/widget.mjs",
      "dest": "/embed/widget.mjs",
      "headers": {
        "Content-Type": "application/javascript",
        "Cache-Control": "public, max-age=3600"
      }
    },
    { "handle": "filesystem" }
  ]
}
